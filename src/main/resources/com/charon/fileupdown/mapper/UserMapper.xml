<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fileupdown.charon.file.dao.UserDao">
    <select id="login" resultType="com.fileupdown.charon.file.entity.User">
        select id,username userName ,password passWord
        from t_user t
        where t.username = #{userName}
    </select>


</mapper>